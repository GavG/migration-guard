{
    "name": "gavg/migration-guard",
    "version": "1.0",
    "description": "Keeping watch of your Laravel Migrations",
    "license": "MIT",
    "keywords": ["laravel", "migrations"],
    "authors": [
        {
            "name": "GavG",
            "email": "gavg.apps@gmail.com"
        }
    ],
    "require": {
      "php": "^7.1",
      "illuminate/database": "*"
    },
    "autoload": {
      "psr-4": {
          "GavG\\MigrationGuard\\": "src"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GavG\\MigrationGuard\\MigrationGuardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
